{
    "name": "{{ debezium_connector_name  }}",
    "config": {
        "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
        "tasks.max": "1",
        "database.hostname": "{{ debezium_connector_db_host }}",
        "database.port": "{{ debezium_connector_db_port }}",
        "database.user": "{{ debezium_connector_db_user }}",
        "database.password": "{{ debezium_connector_db_password }}",
        "database.dbname" : "{{ debezium_connector_db_name }}",
        "database.server.name": "{{ debezium_connector_db_server_name }}",
        "schema.whitelist": "{{ debezium_connector_schema_whitelist }}",
        "table.whitelist" : "{{ debezium_connector_table_whitelist }}",
        "tombstones.on.delete" : "false",
        "transforms" : "router",
        "transforms.router.type" : "io.debezium.transforms.outbox.EventRouter",
        "transforms.router.table.fields.additional.placement": "type:header:eventType",
        "transforms.router.route.topic.replacement": "topic-${routedByValue}-event"
    }
}